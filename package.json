    "body-parser": "^1.19.0",
    "concurrently": "^4.1.2",
    "express": "^4.17.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.14",
    "mongoose": "^5.7.9",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-redux": "^7.1.1",
    "if-env": "^1.0.4",
    "validator": "^11.1.0",
    "react": "^16.6.3",
